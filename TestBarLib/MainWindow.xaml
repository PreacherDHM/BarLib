<Window x:Class="TestBarLib.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestBarLib"
        xmlns:bar="clr-namespace:BarLib.Bars;assembly=BarLib"
        xmlns:graph="clr-namespace:BarLib.Graphs;assembly=BarLib"
        mc:Ignorable="d"
        Background="#333335"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">

            <bar:EfficiencyBarHorizontal Height="30" Margin="10" Width="100"/>
            <graph:LineChart>
                <graph:LineChart.Points>
                    <graph:Point Value="5"/>
                    <graph:Point Value="50"/>
                    <graph:Point Value="500"/>
                    <graph:Point Value="5000"/>
                    <graph:Point Value="50000"/>
                </graph:LineChart.Points>
            </graph:LineChart>

        </StackPanel>
    </Grid>
</Window>
